{
  "name": "starfish-cli",
  "version": "1.1.11",
  "description": "render tool",
  "main": "index.js",
  "scripts": {
    "precompile": "npm run clean",
    "clean": "rm -rf built",
    "compile": "tsc",
    "test": "jest",
    "prepublish": "npm run compile"
  },
  "bin": {
    "starfish": "bin/starfish"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "built"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/built/*"
    ],
    "transform": {
      "^.+\\.(ts|js)$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.(ts)",
      "<rootDir>/src/**/*.test.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/A-Horse/starfish"
  },
  "authorn": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/A-Horse/starfish/issues"
  },
  "homepage": "https://github.com/A-Horse/starfish",
  "dependencies": {
    "bluebird": "^3.3.5",
    "blueimp-md5": "^2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-spawn": "^2.2.3",
    "dateformat": "^1.0.12",
    "ejs": "^2.5.7",
    "fs-extra": "^0.26.5",
    "glob": "^7.0.3",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "markdown": "^0.5.0",
    "meow": "^3.3.0",
    "moment": "^2.24.0",
    "ora": "^1.2.0",
    "org": "^0.2.0",
    "orgpr": "^0.2.3",
    "ramda": "^0.21.0",
    "rimraf": "^2.6.2",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "jest": "^24.1.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^3.4.1"
  }
}
